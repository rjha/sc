mkdir ~/.ssh
cp id_rsa* .ssh/
ls ~/.ssh/
mkdir -p ~/code/github
cd to ~/code/github
clone git repository
    $git clone git@github.com:rjha/sc.git
cd to repository
    $cd sc/
